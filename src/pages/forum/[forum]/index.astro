---
import { allForums } from '../../../content.config';

export async function getStaticPaths() {
    const paths = [];
    for (const forum of allForums) {
        paths.push({ params: { forum }, props: { forum } });
    }

    return paths;
}

const { forum } =  Astro.props; 

return Astro.rewrite(`/forum/${forum}/1`);
