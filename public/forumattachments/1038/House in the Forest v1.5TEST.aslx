    <object name="Little Chest">
      <inherit name="editor_object" />
      <inherit name="container_closed" />
      <inherit name="container_lockable" />
      <hidechildren />
      <listchildren />
      <openmsg>The hinges squeak like it handed been opened for a while.</openmsg>
