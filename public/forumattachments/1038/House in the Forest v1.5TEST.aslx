  <object name="Pony Portal Limbo">
    <inherit name="editor_room" />
    <description type="string"></description>
    <usedefaultprefix type="boolean">false</usedefaultprefix>
    <alias>Inside the Portal</alias>
    <beforeenter type="script">
      menulist = NewStringList()
      list add (menulist, "Earth Pony")
      list add (menulist, "Pegasus")
      list add (menulist, "Unicorn")
      ShowMenu ("What kind of Pony are you?", menulist, false) {
        msg ("--"+ result + "--")
        if (result = "Unicorn") {
