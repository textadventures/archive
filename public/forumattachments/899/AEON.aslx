      MoveObject (player, Accusation)
      SetObjectFlagOff (Profiles, "accuseactive")
      if (GetBoolean(Elodie Parsons, "accused")) {
        msg ("I accuse Elodie Parsons because...")
        list = FormatObjectList("of the evidence in ", accusationbox, "and", ".", false)
