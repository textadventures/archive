              msg ("ELODIE HAS BROWN HAIR. THIS IS NOT A MATCH.")
            }
            msg ("To make another accusation, type \"make accusation\". Type \"accuse [suspect]\" and \"evidence [object]\" before \"complete accusation\".")
          }
          SetObjectFlagOff (Elodie Parsons, "accused")
        }
      }
      else if (GetBoolean(Angel Mitchell, "accused")) {
        msg ("I accuse Angel Mitchell because...")
        list = FormatObjectList("of the evidence in ", accusationbox, "and", ".", false)
        if (list <> "") {
          msg (list)
        }
        SetTimeout (6) {
          msg ("ARE YOU SURE, AMY?")
          msg ("ANGEL HAS SIGNIFICANT DEBT AND DISLIKES RALPH. THAT IS NOT STRONG EVIDENCE.")
          if (GetBoolean(The Elodie Parsons Case, "hairfound")) {
            msg ("ANGEL HAS BLACK HAIR. THIS IS NOT A MATCH.")
          }
          msg ("To make another accusation, type \"make accusation\". Type \"accuse [suspect]\" and \"evidence [object]\" before \"complete accusation\".")
        }
        SetObjectFlagOff (Angel Mitchell, "accused")
      }
      else if (GetBoolean(Ralph Stackhouse Willis, "accused")) {
        msg ("I accuse Ralph Stackhouse Willis because...")
        list = FormatObjectList("of the evidence in ", accusationbox, "and", ".", false)
        if (list <> "") {
          msg (list)
        }
        SetTimeout (6) {
          msg ("THAT DOESN'T MAKE SENSE, AMY.")
          msg ("RALPH HAS NOTHING TO GAIN FROM ELODIE'S KIDNAPPING AND HAS SUSTAINED A SEVERE HEAD INJURY.")
          msg ("To make another accusation, type \"make accusation\". Type \"accuse [suspect]\" and \"evidence [object]\" before \"complete accusation\".")
        }
        SetObjectFlagOff (Ralph Stackhouse Willis, "accused")
      }
      else if (GetBoolean(Bobbie Lawson, "accused")) {
        msg ("I accuse Bobbie Lawson because...")
        list = FormatObjectList("of the evidence in ", accusationbox, "and", ".", false)
        if (list <> "") {
          msg (list)
        }
        SetTimeout (6) {
          if (not Contains (accusationbox,Blonde hair)) {
            msg ("EVIDENCE INSUFFICIENT. TRY AGAIN?")
            msg ("To make another accusation, type \"make accusation\". Type \"accuse [suspect]\" and \"evidence [object]\" before \"complete accusation\".")
          }
          else if (Contains (accusationbox,Blonde hair)) {
            MoveObject (player, Solved Cases)
          }
        }
      }
      firsttime {
        SetTimeout (12) {
          if (not GetBoolean(The Elodie Parsons Case, "hammerfound")) {
            msg ("POLICE SEARCH EVIDENCE")
            MakeObjectVisible (Hammer)
            SetObjectFlagOn (The Elodie Parsons Case, "hammerfound")
            if (not GetBoolean(Blonde hair, "hairfound")) {
              SetObjectFlagOn (Blonde hair, "hairfound")
              MakeObjectVisible (Blonde hair)
            }
            msg ("VIEW NEW EVIDENCE - Y/N?")
            get input {
              if (result="Y") {
                MoveObject (player, The Elodie Parsons Case)
              }
              else if (result<>"Y") {
                msg ("File available in \"The Elodie Parsons Case\"")
              }
            }
          }
          else if (not GetBoolean(The Elodie Parsons Case, "hairfound")) {
            msg ("POLICE SEARCH EVIDENCE")
            MakeObjectVisible (Blonde hair)
            SetObjectFlagOn (The Elodie Parsons Case, "hairfound")
            msg ("VIEW NEW EVIDENCE - Y/N?")
            get input {
              if (result="Y") {
                MoveObject (player, The Elodie Parsons Case)
              }
              else if (result<>"Y") {
                msg ("File available in \"The Elodie Parsons Case\"")
              }
            }
          }
        }
      }
      SetTimeout (20) {
        if (Contains (accusationbox,Blonde hair)) {
          MoveObject (Blonde hair, The Elodie Parsons Case)
        }
        if (Contains (accusationbox,Ransom Demand)) {
          MoveObject (Ransom Demand, The Elodie Parsons Case)
