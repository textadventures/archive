        if (generator.charged = True) {
          msg ("The generator is powered, but the electricty isn't making it to the door. <br/><br/>After a few moments of inspecting the connections, you see a severed wire bundle.")
          MakeObjectVisible (disconnectedwires)
          MakeObjectVisible (engravednote)
          MakeObjectVisible (keypad)
        }
        else if (generator.charged = False) {
          msg ("This old solar generator looks like it might have a chance of working one more time. It just needs energy to convert into electricity.")
        }
