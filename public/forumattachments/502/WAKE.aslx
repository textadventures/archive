      <locked />
    </exit>
    <exit name="city streets" alias="north" to="MediumPathStreets">
      <inherit name="northdirection" />
    </exit>
    <exit name="sewer system" alias="northwest" to="HardPathSewers">
      <inherit name="northwestdirection" />
      <locked />
    </exit>
    <exit name="Medical Station" alias="south" to="MedicalStation">
      <inherit name="southdirection" />
