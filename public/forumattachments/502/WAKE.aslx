      <displayverbs>Look at</displayverbs>
      <poleused type="boolean">false</poleused>
      <useon type="scriptdictionary">
        <item key="Metalpole">
          msg ("You drop the metal pole on the ground; then you careful grab both ends of severed wire and touch the metal pole. Electricity surges through the new connection, and the door's keypad lock powers up.")
          IronDoor.powered = True
        </item>
      </useon>
    </object>
