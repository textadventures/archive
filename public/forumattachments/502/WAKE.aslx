  <command name="Move">
    <pattern>Move #text#</pattern>
    <script><![CDATA[
      if (text = "debris" or text = "aluminum slab" or text = "aluminum" or text = "slab") {
        if (DebrisSlab.lookedat = True) {
          if (DebrisSlab.moved = False) {
            Bone04.taken = True
            AddToInventory (Bone04)
            RemoveObject (DebrisSlab)
            msg ("<br/>You move the slab of aluminum out of the way, and find a bone fragment was behind it.<br/><br/><font size=\"10\" color=\"black\"><i>(Fragment added to inventory)</i></font><br/>")
            DebrisSlab.moved = True
          }
          else {
            msg ("<br/>Either it is not visible, or it cannot be moved.")
          }
        }
        else {
          msg ("<br/>Either it is not visible, or it cannot be moved.")
        }
      }
      else {
        msg ("<br/>Either it is not visible, or it cannot be moved.")
      }
    ]]></script>
  </command>
