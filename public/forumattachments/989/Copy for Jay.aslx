        }
      }
      else if (IsRegexMatch ("(hi|hello|good day|greetings|good morning|good evening)", LCase (result))) {
        msg ("<br/>\"Greetings to you too, space explorer.\"")
        speak_to_android_female
      }
      else if (IsRegexMatch ("(any other|alone|by myself|crew|nobody else|anybody else|anyone else)", LCase (result))) {
        msg ("<br/>\"As I have already explained, we do not have enough space explorers to send you off with a crew, but we have made a deal with one of the mechanics who will join you on Gliese 667 Cc once you are finished with your mission. She goes by the name Victoria Redfield and is quite a capable mechanic. We are stretched thin in our effort to find more chronomite on new planets and there are just not enough space explorers out there to meet our demand.\"")
        speak_to_android_female
      }
      else if (IsRegexMatch ("(colonies|colony)", LCase (result))) {
        msg ("<br/>\"There are currently one bilion people working in the colonies throughout our galaxy. The majority is located on Pluto and Gliese 667 Cc where we found huge deposits of chronomite.\"")
        speak_to_android_female
      }
      else if (IsRegexMatch ("(lack|shortage)", LCase (result))) {
        msg ("<br/>\"As you probably know chronomite is extremely difficult to find and although we currently own rich deposits on Pluto and Gliese 667 Cc, high demand is putting a lot of preassure on our corporation. With such a demand for chronomite the current supplies will dry out within the next 20 years, so we have to expand to other planets and we are stretched thin at the moment.\"")
        speak_to_android_female
      }
      else if (IsRegexMatch ("(fuck|shit|bitch|whore|screw|idiot|stupid|idiotic|fucking|shiting|skank|sex|asshole|ass|butthole|butt|moron|retard|dumb|arse|arsehole|bimbo|bastard|dag|dweeb|deadbeat|dick|dickhead|freak|fruitcake|floosie|floozie|floozy||gasbag|goose|honky|jerk|kook|lardass|lardbutt|lard-butt|lard-ass|lard-arse|limey|loudmouth|metalic breath|bad breath|louse|meathead|old bat|pig|swine|nut|nutter|scum|scumbag|smartass|smart-ass|smartarse|smart-arse|tightass|tight-ass|twit|twat|yokel|airhead|cock|bampot|barmpot|barmy|berk|bint|testicles|bollocks|bugger|piss|vagina|lick|shite|skanky|trollop|dunce|dimwit|tard|fuck ass|fuck arse|fuck-ass|fuck-arse|stooge|faggot|terd|dipshit|dipshite|asswipe|arsewipe|ass-wipe|arse-wipe|drone|dumbass|dumb-ass|dumb-arse|douchebag|douche|douche-bag|mofo|mother fucker|motherfucker|fucker|mother-fucker|dope|fag|mothafucka|fagbag|fag-bag|cunt|pussy|cum|dick-brain|dick-breath|dickbrain|dickbreath|meat-head|metal breath|knob head|turd|fool)", LCase (result))) {
        msg ("<br/>Android female looks at you bitterly. \"That was rude! I have reported your behavior to Chronocorp's conduct manager who will take a fine out of your next salary. According to the conduct protocol 175 section B, I am to inform you about the usage of Astropedia if you have any questions regarding your mission. Also, according to the conduct protocol 176 section A, the insulted party is allowed to cease all communication and will not be fined for insulting back. So, have a good one, asshole!\"")
        wait {
          SetObjectFlagOn (android female, "viconia_angry")
          MoveObject (player, cockpit)
        }
      }
      else {
        msg ("<br/>\"I apologize, but my programming allows me to respond only to topics that are relevant to your current mission.\"")
