  <object name="Create your character">
    <inherit name="editor_room" />
    <descprefix type="string"></descprefix>
    <objectslistprefix type="string"></objectslistprefix>
    <exitslistprefix type="string"></exitslistprefix>
    <description type="string"></description>
    <usedefaultprefix type="boolean">false</usedefaultprefix>
    <enter type="script">
      msg ("What is your name?")
      get input {
        ClearScreen
        player.alias = result
        msg ("Very well, " + player.alias + "." + " How old are you" + "?")
        get input {
          ClearScreen
          player.age = result
          msg ("Now please type your height in centimeters.")
          get input {
            ClearScreen
            player.height = result
            msg ("And how much do you weigh in kilograms?")
            get input {
              ClearScreen
              player.weight = result
              ShowMenu ("Are you a male or a female?", Split ("Male;Female", ";"), false) {
                ClearScreen
                player.gender = result
                ShowMenu (" Now choose your race. " + " This will determine your general appearance and skin tone.", Split ("European;African;Asian;Arab;Amerindian", ";"), false) {
                  player.race = result
                  ClearScreen
                  ShowMenu ("Now please describe the color of your hair.", Split ("Black;Light brown;Dark brown;Blonde;Gray;Red;Green;Purple;Blue", ";"), false) {
                    player.hairc = result
                    ClearScreen
                    ShowMenu ("How long is your hair?", Split ("Bald;Short;Medium length;Long", ";"), false) {
                      player.hairl = result
                      ClearScreen
                      ShowMenu ("Now please describe your eye color.", Split ("Blue;Green;Brown;Gray", ";"), false) {
                        player.eye = result
                        ClearScreen
                        msg ("")
                        if (player.hairl="Bald") {
                          ClearScreen
                          msg ("You successfully created your character. Your name is " + player.alias + " and you are a " + LCase (player.age) + " year old " + LCase (player.gender) + "."  + " With your " + LCase (player.height) + " cm, " + LCase (player.weight) + " kg and " + LCase (player.eye) + " eyes you look like an average person of " + LCase (player.race) + " origin. One thing to note however is that you are bald, but even with no hair you are still an attractive individual as if you were born to wear such a look.")
                          msg ("")
                          msg ("You are finished. Press a key to continue.")
                          wait {
                            MoveObject (player, main deck)
                          }
                        }
                        else {
                          ClearScreen
                          msg ("You successfully created your character. Your name is " + player.alias + " and you are a " + LCase (player.age) + " year old " + LCase (player.gender) + "."  + " With your " + LCase (player.height) + " cm, " + LCase (player.weight) + " kg and " + LCase (player.eye) + " eyes you look like an average person of " + LCase (player.race) + " origin. Another thing to note is that your " + LCase (player.hairl) + " " + LCase (player.hairc) + " hair gives nice facial structure to your otherwise symmetrical face.")
                          msg ("")
                          msg ("You are finished. Press a key to continue.")
                          wait {
                            MoveObject (player, main deck)
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    </enter>
