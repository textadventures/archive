  <function name="InitInterface">
    request (FontName, game.defaultfont)
    request (FontSize, ToString(game.defaultfontsize))
    request (Background, game.defaultbackground)
    request (Foreground, game.defaultforeground)
    request (LinkForeground, game.defaultlinkforeground)
    request (SetCompassDirections, Join(game.compassdirections, ";"))
    request (SetInterfaceString, "InventoryLabel=Inventario")
    request (SetInterfaceString, "PlacesObjectsLabel=Lugares y Objetos")
    request (SetInterfaceString, "CompassLabel=Brújula")
    request (SetInterfaceString, "InButtonLabel=Entrar")
    request (SetInterfaceString, "OutButtonLabel=Salir")
    request (SetInterfaceString, "EmptyListLabel=(vacío)")
    request (SetInterfaceString, "NothingSelectedLabel=(nada seleccionado)")
    if (game.useframe) {
      request (ClearScreen, "")
      insert ("Frame.htm")
      request (RunScript, "beginUsingTextFrame")
    }
    if (game.showpanes) {
      request (Show, "Panes")
    }
    else {
      request (Hide, "Panes")
    }
