  <function name="Mixen" parameters="object1, object2">
    if (HasAttribute(object1, "mix")) {
      if (HasAttribute(object2, "mix")) {
        if (object1.mix = object2.mix) {
          msg ("Deze dingen kun je niet mixen.")
        }
        else if (object1.parent = object2.parent) {
          if (not Contains (game,paars borrelend drankje)) {
            firsttime {
              msg ("Je mixt " +object1.alias+ " met "+object2.alias+ " in " +object1.parent.alias+ ". Het bruist een beetje. Er ontstaat een paarsig, borrelend drankje.")
              MoveObject (paars borrelend drankje 2, object1.parent)
              RemoveObject (object1)
              RemoveObject (object2)
            }
          }
          else {
            msg ("Je mixt " +object1.alias+ " met "+object2.alias+ " in " +object1.parent.alias+ ". Het bruist een beetje. Er ontstaat een paarsig, borrelend drankje.")
            MoveObject (paars borrelend drankje, object1.parent)
            RemoveObject (object1)
            RemoveObject (object2)
          }
        }
        else {
          msg ("Helaas, " +object1.alias+ " en "+object2.alias+ " bevinden zich niet in dezelfde container.")
        }
      }
      else {
        msg (object2.alias+ " kan niet gemixt worden.")
      }
    }
    else {
      msg (object1.alias+ " kan niet gemixt worden.")
    }
  </function>
