  <command name="lees">
    <pattern>lees #object#</pattern>
    <unresolved>Dat gaat niet.</unresolved>
    <script>
      if (Got(object)) {
        if (HasAttribute(object, "lees")) {
          do (object, "lees")
        }
        else {
          msg ("Je kunt "  + object.name+  "  niet lezen.")
        }
      }
      else {
        if (object.name="briefje1") {
          do (briefje1, "lees")
        }
        else {
          msg ("Je hebt geen " + object.name+  ".")
        }
      }
    </script>
  </command>
  <command name="druk">
    <pattern>druk #object#</pattern>
    <script>
      if (HasAttribute(object, "druk")) {
        do (object, "druk")
