              msg ("Het water warmt op....En begint te koken.")
              RemoveObject (halve liter water)
              MoveObject (kokend water, ketel)
            }
            EnableTurnScript (voegbloedmuggentoe)
            DisableTurnScript (kookwater)
          }
        }
      </script>
