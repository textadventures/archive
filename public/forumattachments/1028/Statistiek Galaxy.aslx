      <script>
        if (Contains (ketel,bloedmuggen)) {
          firsttime {
            msg ("De bloedmuggen drijven in het water. Een voor een spatten de bloedmuggen uiteen. Ze kleuren de toverdrank rozig.")
            RemoveObject (kokend water)
            RemoveObject (bloedmuggen)
            MoveObject (rozige kokende vloeistof, ketel)
          }
          EnableTurnScript (voegnaaktslakkentoe)
          DisableTurnScript (voegbloedmuggentoe)
        }
      </script>
    </turnscript>
    <turnscript name="voegnaaktslakkentoe">
      <script>
        if (Contains(ketel, naaktslakken)) {
          if (naaktslakken.gesneden) {
            firsttime {
              msg ("De stukjes naaktslak lossen op in de roze vloeistof en maken het tot een vieze roze blubber")
              RemoveObject (naaktslakken)
              RemoveObject (rozige kokende vloeistof)
              MoveObject (rozige kokende blubber, ketel)
            }
            EnableTurnScript (voegzwiepriettoe)
            DisableTurnScript (voegnaaktslakkentoe)
          }
        }
      </script>
    </turnscript>
    <turnscript name="voegzwiepriettoe">
      <script>
        if (Contains (ketel,zwiepriet)) {
          if (zwiepriet.gevlochten) {
            firsttime {
              msg ("Het gevlochten zwiepriet lost op in het roze water, en de kleur veranderd naar bruin.")
              RemoveObject (rozige kokende blubber)
              RemoveObject (zwiepriet)
              MoveObject (bruine drab, ketel)
            }
            EnableTurnScript (voeghuidtoe)
            DisableTurnScript (voegzwiepriettoe)
          }
        }
      </script>
    </turnscript>
    <turnscript name="voeghuidtoe">
