            if (result="chi-kwadraat voor samenhang") {
              IncreaseScore (1)
              msg ("Zizzl knikt instemmend")
            }
            else {
              msg ("Het betreft hier twee nominale variabelen (kandidaat en stemwijze), waarvan we met een chi-kwadraat voor samenhang zullen nagaan of er sprake is van een verband.")
            }
            wait {
              ClearScreen
              nulh = NewStringList()
