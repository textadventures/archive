        set (portable transporter, "formerroom", game.pov.parent)
      </formerroom>
      <object name="groene knop">
        <inherit name="editor_object" />
        <alt type="stringlist">
          <value>knop</value>
        </alt>
        <displayverbs type="stringlist">
          <value>Druk</value>
        </displayverbs>
        <useindividualverblist type="boolean">false</useindividualverblist>
        <drop type="boolean">false</drop>
        <usestandardverblist />
        <scenery type="boolean">false</scenery>
        <take type="boolean">false</take>
        <usedefaultprefix />
        <inventoryverbs type="stringlist">
          <value>Druk</value>
        </inventoryverbs>
        <gender>de groene knop</gender>
        <article>de groene knop</article>
        <druk type="script">
          ClearScreen
          SetFramePicture ("teleportatie.jpg")
          play sound ("beam.mp3", true, false)
          ClearFramePicture
          ClearScreen
          if (not game.pov.parent = Archieven) {
            msg ("Je hebt je met de portable transporter verplaatst naar de Archieven")
            set (portable transporter, "formerroom", game.pov.parent)
            MoveObject (game.pov, Archieven)
          }
          else {
            msg ("Je hebt je met de portable transporter verplaatst naar " +portable transporter.formerroom.name)
            MoveObject (game.pov, portable transporter.formerroom)
            set (portable transporter, "formerroom", Archieven)
          }
        </druk>
        <ontake type="script">
          AddToInventory (portable transporter)
        </ontake>
        <ondrop type="script">
          MoveObject (portable transporter, game.pov.parent)
        </ondrop>
      </object>
    </object>
