        <turnon type="script">
          if (ListContains(ScopeReachable(), this)) {
            if (this.switchedon) {
              msg (DynamicTemplate("AlreadySwitchedOn", this))
            }
            else {
              if (Got(toverstok)) {
                msg (this.switchonmsg)
                this.switchedon = true
              }
              else {
                msg ("Dit is een school voor magie, misschien heb je een toverstok nodig? Zoek 'm op en probeer het nog eens.")
              }
            }
