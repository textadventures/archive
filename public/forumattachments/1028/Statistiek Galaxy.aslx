        </inventoryverbs>
        <lees type="script"><![CDATA[
          SetFontName ("'Comic Sans MS', cursive, sans-serif")
          SetForegroundColour ("DarkBlue")
          msg (player.playername + ", ")
          msg ("<br/>Ik hoop dat je inmiddels de bewaker met de hoogste intelligentie een slaappil hebt gegeven, en de andere pil nog hebt bewaard.<br/><br/>")
          wait {
            msg ("Die slaappil moet je op dezelfde wijze geven aan de bewaker, die het beste zijn werk uitvoert.<br/>")
            wait {
              msg ("Je moet weten, dat het de bewakers ten strengste verboden is, iets anders te doen dan \"bewaken\". <br/>")
              wait {
                msg ("Dat nemen ze echter niet zo nauw! Allemaal spelen ze graag wel eens een spelletje")
                DisplayHttpLink ("\"Kadis-kot.\"", "http://database.obsidianfleet.net/wiki/index.php?title=Kadis-kot", false)
                msg ("Met elkaar, of met een voorbijganger.")
                wait {
                  msg ("Je moet uitzoeken of er een verschil is tussen de twee bewakers die over zijn gebleven, in hoe vaak ze zich laten verleiden tot het spelen van Kadis-Kot.")
                  wait {
                    msg ("Degene die het <u>minst aantal spelletjes speelt</u>, doet het best zijn werk. Die kun je beter maar met de slaappil uit de weg ruimen.")
                    wait {
                      msg ("Hier zijn de gegevens van hoeveel keer de bewakers zich het afgelopen jaar hebben laten verleiden tot Kadis-Kot:<br/><br/>Mookoozl: 130 keer<br/>Pakkoelt: 147 keer<br/>Zlootie: 113 keer<br/><br/>Dat was het!<br/><br/>Groetjes, Zizzl.")
                      wait {
                        msg ("P.S. Misschien moet je het wel even met een Chi-kwadraat analyse toetsen!!!! Op 5 % sign. niveau en rond steeds af op 2 decimalen.")
                        SetForegroundColour ("Black")
                        SetFontName ("Verdana, Geneva, sans-serif")
                        msg ("")
                      }
                    }
                    msg ("")
                  }
                }
              }
            }
          }
        ]]></lees>
      </object>
    </object>
