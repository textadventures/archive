    <object name="beeldscherm">
      <inherit name="editor_object" />
      <alt type="stringlist">
        <value>beeldscherm</value>
        <value>scherm</value>
      </alt>
      <scenery type="boolean">false</scenery>
      <visible />
      <gender>het beeldscherm</gender>
      <article>het beeldscherm</article>
      <look type="script">
        if (IsSwitchedOn(computer)) {
          if (GetBoolean(player, "opdracht1")) {
            opdrachteen
          }
          else if (GetBoolean(player, "opdracht2")) {
            opdrachttwee
          }
          else if (GetBoolean(player, "opdracht3")) {
            opdrachtdriestart
          }
          else {
            ClearScreen
            TextFX_Typewriter ("Wachtwoord:", 200)
            get input {
              if (LCase(result)= "waterstofmoleculen") {
                if (not GetBoolean(computer, "firstlogin")) {
                  IncreaseScore (5)
                  SetObjectFlagOn (computer, "firstlogin")
                  computeronderzoeksruimte
                }
                else {
                  computeronderzoeksruimte
                }
              }
              else {
                msg ("Dat was blijkbaar niet het goede wachtwoord. De computer heeft zichzelf automatisch uitgeschakeld. Misschien moet je eerst op zoek naar het juiste wachtwoord.")
                SwitchOff (computer)
              }
            }
          }
        }
        else {
          msg ("Je ziet een zwart scherm. De computer staat waarschijnlijk uit.")
        }
      </look>
    </object>
    <object name="portable transporter">
      <inherit name="editor_object" />
