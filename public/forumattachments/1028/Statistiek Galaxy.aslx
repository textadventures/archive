          if (ListContains(ScopeReachable(), this)) {
            if (not this.switchedon) {
              msg (DynamicTemplate("AlreadySwitchedOff", this))
            }
            else {
              if (Got(toverstok)) {
                msg (this.switchoffmsg)
                this.switchedon = false
              }
              else {
