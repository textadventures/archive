            ClearScreen
            picture ("opdracht1deel1.jpg")
            wait {
              ClearScreen
              picture ("opdracht1deel2.jpg")
              wait {
                ClearScreen
                picture ("opdracht1 deel3.jpg")
                wait {
                  ClearScreen
                  picture ("opdracht1 slot.jpg")
                }
              }
            }
          }
        }
        else {
          ClearScreen
          picture ("opdracht1deel1.jpg")
          wait {
            ClearScreen
            picture ("opdracht1deel2.jpg")
            wait {
              ClearScreen
              picture ("opdracht1 deel3.jpg")
              wait {
